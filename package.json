{
  "name": "resume",
  "version": "1.0.0",
  "description": "My Resume ",
  "main": "index.js",
  "scripts": {
    "test": "resume test ./config/resume.json",
    "build-pdf": "npm run styles && fluentcv BUILD config/resume.json to build/resume.pdf -t jsonresume-theme-class",
    "build-pdf-html": "fluentcv BUILD config/resume.json to build/index.html -t jsonresume-theme-class",
    "start": "onchange \"scss/**/*\" \"config/*\" \"jsonresume-theme-class/*\" -- npm run start-pdf",
    "styles": "node-sass scss/style.scss -o build && cp build/style.css json-resume-theme-class",
    "start-pdf": "npm run build-pdf-html && npm run styles && budo --dir build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nascherman/resume.git"
  },
  "keywords": [
    "resume",
    "nascherman"
  ],
  "author": "nascherman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nascherman/resume/issues"
  },
  "homepage": "https://github.com/nascherman/resume#readme",
  "devDependencies": {
    "budo": "^9.2.2",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "fluentcv": "^1.8.1",
    "node-sass": "^4.7.2",
    "onchange": "^3.0.2",
    "sass-lint": "^1.12.1",
    "sass-lint-config-entoj": "^1.0.1",
    "wkhtmltopdf": "^0.3.3"
  },
  "dependencies": {
    "handlebars": "^4.0.6"
  }
}
